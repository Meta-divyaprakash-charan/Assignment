package search;

import static org.junit.jupiter.api.Assertions.assertEquals;
import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;

/**
 * @author Divyaprakash
 *
 */
class TestSearch{
	
	@Test
	public void testBinary(){
		Search s = new Search();
		int[] input = {1,4,6,3,5,2};
	assertEquals(4, s.binarySearch(input, 5));	
	}
	
	@Test
	public void testLinear(){
		Search s = new Search();
		int [] a = {2,4,6,78,9};
		assertEquals(-1, s.linearSearch(a, 9));
	}
}
