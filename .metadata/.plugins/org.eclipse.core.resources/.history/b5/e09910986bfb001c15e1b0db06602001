package search;
import java.util.*;
public class Search{
	int linearSearch(int[] array, int element){
		for(int i = 0;i<array.length;i++){
			if(array[i]==element){
				return i;
			}
		}
		return -1;
	}
	
	int binarySearch(int[] array, int element){
		Arrays.sort(array);
		int start = 0;
		int end = array.length-1;
		while(start<end)
		{	int i = end/2;
			if(array[i]==element){
				return i;
			}
			else if(array[i]<element){
				start = i;		
			}
			else{
				end = i;
			}
		}
		return -1;
	}
}