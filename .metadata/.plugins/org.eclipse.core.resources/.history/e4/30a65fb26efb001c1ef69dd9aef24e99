package search;

import static org.junit.jupiter.api.Assertions.assertEquals;
import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;

/**
 * @author Divyaprakash
 *
 */
class TestSearch{
	
	
	@Test
	void testBinary(){
		Search s = new Search();
		int[] input = {1,4,6,3,5,2};
	assertEquals(4, s.binarySearch(input, 5));	
	}
	
	@Test
	void testLinear(){
		Search s = new Search();
		int [] input = {2,4,5,6,7,8,3};
		assertEquals(4, s.linearSearch(input, 7));
	}

}
