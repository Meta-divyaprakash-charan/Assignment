
public class fcfs {
	int waitingTime[];
	int turnAroundTime[];
	int compTime[];
	void sort(int[][] arr, int n){
		int temp = 0;
		for(int i=0;i<n-1;i++){
				if(arr[i+1][0]<arr[i][0]){
					temp = arr[i][0];
					arr[i][0] = arr[i+1][0];
					arr[i+1][0] = temp;
			}
		}
	}
