
public class fcfs {
	int waitingTime[];
	int turnAroundTime[];
	int compTime[];
	void sort(int[][] arr, int n){
		int temp1= 0;
		int temp2= 0;
		for(int i=0;i<n-1;i++){
			for(int j=i;j<n-1;j++) {
				if(arr[i+1][0]<arr[i][0]){
					temp1 = arr[i][0];
					temp2 = arr[i][1];
					arr[i][0] = arr[i+1][0];
					arr[i][1] = arr[i+1][1];
					arr[i+1][0] = temp1;
					arr[i+1][1] = temp2;					
			}}
		}
	}
void completionTime(){
		
	}
	
void turnAroundTime(){
			
	}

void waitingTime(){
	
}

int avgWaitingTime(){
	return 0;
}

int maxWaitingTime(){
	return 0;
}
}
