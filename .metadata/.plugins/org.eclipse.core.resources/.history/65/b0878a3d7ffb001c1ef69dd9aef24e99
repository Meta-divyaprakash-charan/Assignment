package search;

import static org.junit.jupiter.api.Assertions.assertEquals;
import org.junit.jupiter.api.Test;

/**
 * @author Divyaprakash
 *
 */
class TestSearch{
	
	@Test
	public void testLinear(){
		Search s = new Search();
		int [] a = {2,4,6,78,9};
		assertEquals(-1, s.linearSearch(a, 0));
	}
	
	@Test
	void test(){
		Search s = new Search();
		int [] a = {1,2,3,4,5,6};
		assertEquals(5, s.binarySearch(a, 6));
 	}
	
}
