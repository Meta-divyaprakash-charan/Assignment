package calculate;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;

/**
 * @author Divyaprakash
 *
 */
class TestClass {
	recursion r;
	@BeforeEach
	void init(){
		r = new recursion();
	}
	
	@Test
	void testLcm() {
		int[][] input = {{2,3},{20,36},{45,9}};
		int[] output = {6,180,45};
		for(int i=0;i<input.length;i++){
			assertEquals(output[i], r.calculateLCM(input[i][0], input[i][1]));
		}
	}

}
